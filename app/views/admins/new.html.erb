<%= form_for(@admin) do |f| %>
  <%= f.label :first %>:
  <%= f.text_field :first %><br />

  <%= f.label :last %>:
  <%= f.text_field :last %><br />

  <%= f.label :email %>:
  <%= f.email_field :email %><br />

  <%= f.label :username %>:
  <%= f.text_field :username %><br />

  <%= f.label :password %>:
  <%= f.password_field :password %><br />

  <%= f.label :password_confirmation %>:
  <%= f.password_field :password_confirmation %><br />

  <%= f.submit %>
<% end %>
